<Expression> ::= <Multiply> <PlusOpt>;
<PlusOpt> ::= "+" <Multiply><PlusOpt> | "-" <Multiply><PlusOpt> | null;
<Multiply> ::= <PorN> <MultiplyOpt>;
<MultiplyOpt> ::= "*" <PorN> <MultiplyOpt> | "/" <PorN> <MultiplyOpt> | null;
<PorN> ::= "-" <Positive> | <Positive>;
<Positive> ::= <Base> <Power>;
<Power> ::= "^" <Positive> | null;
<Base> ::= "(" <Expression> ")" | number;